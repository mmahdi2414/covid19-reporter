<html>
    <head>
        <title>Covid-19 Reporter</title>
    </head>
    <body>
        <table>
            <tr>
               <th>Name</th>
               <th>Population</th>
               <th>Confirmed Cases</th>
               <th>Deaths</th>
               <th>Recovered</th>
            </tr>
         
            <% for (var i = 0; i < data.length; /* I save the data in a variable 'quotation', I don't know how you named your variable  */  i++) { %>
             <tr>
               <td><%= data[i].country %></td>
               <td><%= data[i].country_population %></td>
               <td><%= data[i].latest.confirmed %></td>
               <td><%= data[i].latest.deaths %></td>
               <td><%= data[i].latest.recovered %></td>
             </tr>    
            <% } %>
         </table>
    </body>
</html>